{
	"//$schema": "https://www.schemastore.org/package",
	"scripts": {
		"build": "npm run compile && npm run compile-web && npm run generate-wasm && node ./out/Onigmo/moveOnigmo",
		"package": "vsce package",
		"watch": "tsc -watch -p ./",
		"watch-web": "webpack --watch",
		"compile": "tsc -p ./",
		"compile-web": "webpack --no-devtool --no-watch --mode production",
		"generate-wasm": "cd ./src/tree-sitter/tree-sitter-json/ && tree-sitter generate && tree-sitter build --wasm --output ../../../out/tree-sitter-jsontm.wasm && cd ./../tree-sitter-regex/ && tree-sitter generate && tree-sitter build --wasm --output ../../../out/tree-sitter-regextm.wasm",
		"open-in-browser": "vscode-test-web --extensionDevelopmentPath=. ."
	},
	"contributes": {
		"languages": [
			{
				"filenamePatterns": [
					"**/syntaxes/*inject*.json",
					"**/syntaxes/*codeblock*.json",
					"**/syntaxes/*code?block*.json",
					"**/syntaxes/*.tmLanguage.json",
					"**/syntaxes/*.tmLanguage copy{, [0-9]*}.json",
					"**/syntaxes/*.tmLanguage{, copy} - Copy{, ([0-9]*)}{, - Copy*}.json",
					"**/grammars/*inject*.json",
					"**/grammars/*codeblock*.json",
					"**/grammars/*code?block*.json",
					"**/grammars/*.tmLanguage.json",
					"**/grammars/*.tmLanguage copy{, [0-9]*}.json",
					"**/grammars/*.tmLanguage{, copy} - Copy{, ([0-9]*)}{, - Copy*}.json",
					"* ** *** {*} {**} {***} {*,**} [*] [**] [***]",
					"? {?} {?,?} [?]",
					"[\/ / /-/ ] [/]] [/^\/]/] []a[!] [A-Fa-f0-9] []-] [--0] [--0] [.-0] [---] [!]a-] [[?*\\]",
					"**/{Crusader Kings III,CK,CK3,1158310}/**/*.{txt,gui,info}",
					"**/{Europa Universalis IV,EU,EU4,236850}/**/*.{txt,gui}",
					"**/{Hearts of Iron IV,HOI,HOI4,394360}/**/*.{txt,gui}",
					"**/{Victoria 3,VIC,VIC3,529340}/**/*.{txt,gui,info}",
					"**/*requirements*.{txt, in}",
					"**/*constraints*.txt",
					"**/requirements/*.{txt,in}",
					"**/constraints/*.txt",
					"@vcode-tm-log-*-*-*-*-*.txt",
					"**/*.[w.-]+",
					"**/([w.-]+)/?",
					"{**/*?[w.-]+/?(,**/*?[w.-]+/?)*}",
					"{**/*?[w.-]+(/(**)?)?(,**/*?[w.-]+(/(**)?)?)*}",
					"**((/[w.-]+)+)/?",
					"([w.-]+(/[w.-]+)*)/?",
					"Dockerfile.*",
					"Containerfile.*",
					"**/rebase-merge/done",
					"**/.config/git/config",
					"**/.git/config",
					"**/User/snippets/*.json",
					"**/User/profiles/*/snippets/*.json",
					"**/snippets*.json",
					"**/.cursor/**/*.mdc",
					"**/.github/agents/*.md",
					"tsconfig.*.json",
					"jsconfig.*.json",
					"tsconfig-*.json",
					"jsconfig-*.json",
					"compose.yml",
					"compose.yaml",
					"compose.*.yml",
					"compose.*.yaml",
					"*docker*compose*.yml",
					"*docker*compose*.yaml",
					"**/templates/**/*.html",
					"**/templates/**/*",
					"**/test/integration/**/*.rb",
					"**/controls/*.rb",
					"**/recipes/*.yml",
					"**/recipes/*.yaml",
					"*suite*.rc",
					"suite*.rc.*",
					"flow.rc",
					"flow-tests.rc",
					"@vcode-tm-log-*-*-*-*-*.txt"
				],
				"firstLine": "{(\"scopeName\"|\"(information_for_contributors|fileTypes)\":\\[)",
				"configuration": "./language-configuration.json",
				"icon": {
					"dark": "assets/TextMate-file-icon.svg",
					"light": "assets/TextMate-file-icon.svg"
				}
			}
		],
		"grammars": [
			{
				"language": "json-textmate",
				"scopeName": "source.json.textmate",
				"path": "./syntaxes/json.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.json.textmate.regexp": "json-textmate-regex"
				},
				"tokenTypes": {
					"abc.def": "string"
				},
				"unbalancedBracketScopes": [
					"invalid.illegal.expected-dictionary-separator.json",
					"invalid.illegal.expected-array-separator.json",
					"invalid.illegal.expected-value-separator.json"
				]
			},
			{
				"language": "json-textmate-regex",
				"scopeName": "source.json.textmate.regexp",
				"path": "./syntaxes/regex.tmLanguage.json"
			},
			{
				"language": "yaml-textmate",
				"scopeName": "source.yaml.textmate",
				"path": "./syntaxes/yaml.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.json.textmate.regexp": "json-textmate-regex",
					"meta.embedded.yaml.textmate.regexp": "json-textmate-regex"
				},
				"unbalancedBracketScopes": [
					"invalid.illegal",
					"storage.type.tag.shorthand.yaml",
					"keyword.control.flow"
				]
			},
			{
				"scopeName": "source.json.snippets",
				"path": "./syntaxes/schema-defaultSnippets.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.json.comments.snippets.snippets": "snippets-snippets",
					"meta.embedded.json.comments.snippets": "snippets",
					"meta.embedded.json.textmate.regexp": "json-textmate-regex"
				},
				"injectTo": [
					"source.json.embedded"
				]
			}
		],
		"breakpoints": [
			{
				"language": "",
				"when": "a =~ /a\//"
			},
			{
				"language": "",
				"when": "true == \"false && true == \/false && true == \\false && true == \nfalse && true == @fals && true == afalse"
			}
		],
		"chatParticipants": [
			{
				"commands": [
					{
						"description": "",
						"name": "",
						"when": "true"
					}
				],
				"description": "Chat participant for GitHub Pull Requests extension",
				"fullName": "GitHub Pull Requests",
				"id": "githubpr",
				"name": "githubpr",
				"when": "config.githubPullRequests.experimental.chat"
			}
		],
		"chatViewsWelcome": [
			{
				"when": "true && '"
			},
			{
				"when": "true && '\""
			},
			{
				"when": "true && '\\"
			},
			{
				"when": "true && ''"
			},
			{
				"when": "true ) ~ = < > ) > ( & % && /"
			},
			{
				"when": "true && //"
			},
			{
				"when": "true && /\""
			},
			{
				"when": "true && /\\"
			}
		],
		"commands": {
			"command": "abc",
			"title": "ABC",
			"enablement": "dfd > .1true"
		},
		"commands2": [
			{
				"command": "",
				"enablement": "a && a && a =~ /"
			},
			{
				"command": "prettier.forceFormatDocument",
				"title": "%ext.command.forceFormatDocument.title%",
				"when": "editorFocus"
			}
		],
		"configuration": {
			"properties": {
				"cmake.cpackArgs": {
					"default": [],
					"description": "%cmake-tools.configuration.cmake.cpackArgs.description%",
					"items": {
						"type": "string"
					},
					"scope": "resource",
					"type": "array",
					"when": "useCMakePresets == true"
				},
				"cmake.cpackEnvironment": {
					"additionalProperties": {
						"description": "%cmake-tools.configuration.cmake.environment.additionalProperties.description%",
						"type": "string"
					},
					"default": {},
					"description": "%cmake-tools.configuration.cmake.cpackEnvironment.description%",
					"scope": "resource",
					"type": "object",
					"when": "useCMakePresets == true"
				},
				"cmake.cpackPath": {
					"default": null,
					"description": "%cmake-tools.configuration.cmake.cpackPath.description%",
					"scope": "resource",
					"type": "string",
					"when": "useCMakePresets == true"
				}
			},
			"title": "CMake Tools",
			"type": "object"
		},
		"continueEditSession": [
			{
				"command": "",
				"when": "true"
			}
		],
		"debuggers": [
			{
				"type": "clr",
				"when": "workspacePlatform == windows"
			}
		],
		"debugVisualizers": [
			{
				"id": "inlineHexDecoder",
				"when": "debugConfigurationType == 'debugpy' && (variableType == 'float' || variableType == 'int')"
			}
		],
		"keybindings": {
			"command": "",
			"key": "",
			"when": "true"
		},
		"keybindings2": [
			{
				"command": "",
				"key": "",
				"when": "true"
			}
		],
		"languageModelTools": [
			{
				"canBeReferencedInPrompt": true,
				"displayName": "%c_cpp.languageModelTools.configuration.displayName%",
				"icon": "$(file-code)",
				"modelDescription": "For the active C or C++ file, this tool provides: the language (C, C++, or CUDA), the language standard version (such as C++11, C++14, C++17, or C++20), the compiler (such as GCC, Clang, or MSVC), the target platform (such as x86, x64, or ARM), and the target architecture (such as 32-bit or 64-bit).",
				"name": "cpptools-lmtool-configuration",
				"toolReferenceName": "cpp",
				"userDescription": "%c_cpp.languageModelTools.configuration.userDescription%",
				"when": "(config.C_Cpp.experimentalFeatures =~ /^[eE]nabled$/)"
			}
		],
		"menus": {
			"chat/modelPicker": [
				{
					"command": "",
					"when": "== /abf/"
				},
				{
					"command": "",
					"when": "=~ /abf/"
				},
				{
					"command": "",
					"when": "scmHistoryItemRef =~ /abf/"
				},
				{
					"command": "",
					"when": "scmProvider == git && scmHistoryItemRef =~ /^refs\\/heads\\/|^refs\\/remotes\\//"
				}
			],
			"commandPalette": [
				{
					"command": "gitlens.clearFileAnnotations",
					"when": "(resource in gitlens ) "
				},
				{
					"command": "gitlens.clearFileAnnotations",
					"when": "resource in gitlens:tabs:blameable && (gitlens:window:annotated || resource in gitlens:tabs:annotated)"
				},
				{
					"command": "gitlens.computingFileAnnotations",
					"group": "navigation@100",
					"when": "resource in gitlens:tabs:blameable && (gitlens:window:annotated == computing || resource in gitlens:tabs:annotated:computing) && config.gitlens.menus.editorGroup.blame"
				},
				{
					"command": "gitlens.clearFileAnnotations",
					"group": "navigation@100",
					"when": "resource in gitlens:tabs:blameable && (gitlens:window:annotated == computed || (resource in gitlens:tabs:annotated && resource not in gitlens:tabs:annotated:computing)) && config.gitlens.menus.editorGroup.blame"
				}
			],
			"diffEditor/gutter/selection": [
				{
					"command": "",
					"when": "dfd"
				}
			],
			"editor/context": [
				{
					"command": "typescript.goToSourceDefinition",
					"group": "navigation@1.41",
					"when": "(==)"
				},
				{
					"command": "typescript.goToSourceDefinition",
					"group": "navigation@1.41",
					"when": "tsSupportsSourceDefinition && (resourceLangId == typescript || resourceLangId == typescriptreact || resourceLangId == javascript || resourceLangId == javascriptreact)"
				}
			],
			"editor/title": [
				{
					"command": "godotTools.debugger.pinFile",
					"group": "navigation@11",
					"when": "editorLangId in godotTools.context.sceneLikeFiles && !isInDiffEditor && !virtualWorkspace && !(resourcePath in godotTools.context.pinnedScene)"
				},
				{
					"command": "godotTools.debugger.unpinFile",
					"group": "navigation@11",
					"when": "editorLangId in godotTools.context.sceneLikeFiles && !isInDiffEditor && !virtualWorkspace && (resourcePath in godotTools.context.pinnedScene)"
				}
			],
			"explorer/context": [
				{
					"command": "godotTools.debugger.pinFile",
					"group": "2_workspace",
					"when": "resourceLangId in godotTools.context.sceneLikeFiles && !(resourcePath in godotTools.context.pinnedScene)"
				},
				{
					"command": "godotTools.debugger.unpinFile",
					"group": "2_workspace",
					"when": "resourceLangId in godotTools.context.sceneLikeFiles && (resourcePath in godotTools.context.pinnedScene"
				}
			],
			"gitlens/editor/annotations": [
				{
					"command": "gitlens.clearFileAnnotations",
					"group": "1_gitlens@1",
					"when": "resource in gitlens:tabs:blameable && (gitlens:window:annotated || resource in gitlens:tabs:annotated)"
				}
			]
		},
		"taskDefinitions": [
			{
				"properties": {
					"args": {
						"description": "%grunt.taskDefinition.args.description%",
						"type": "array"
					},
					"file": {
						"description": "%grunt.taskDefinition.file.description%",
						"type": "string"
					},
					"task": {
						"description": "%grunt.taskDefinition.type.description%",
						"type": "string"
					}
				},
				"required": [
					"task"
				],
				"type": "grunt",
				"when": "shellExecutionSupported"
			}
		],
		"views": {
			"tes": [
				{
					"icon": "",
					"id": "",
					"name": "",
					"when": "true"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"icon": "panel-icon.svg",
					"id": "hexExplorer",
					"title": "%name%",
					"when": "hexEditor:showSidebarInspector"
				}
			]
		},
		"viewsWelcome": [
			{
				"contents": "",
				"enablement": "true",
				"view": "",
				"when": "true"
			}
		],
		"walkthroughs": [
			{
				"description": "%walkthrough.pythonWelcome.description%",
				"id": "pythonWelcome",
				"steps": [],
				"title": "%walkthrough.pythonWelcome.title%",
				"when": "workspacePlatform != webworker"
			},
			{
				"description": "%pascal.walkthroughs.description%",
				"id": "pascalWelcome",
				"steps": [
					{
						"description": "%pascal.walkthroughs.formatter.description%",
						"id": "formatter",
						"media": {
							"markdown": "walkthrough/formatter.md"
						},
						"title": "%pascal.walkthroughs.formatter.title%",
						"when": "!isWeb"
					}
				],
				"title": "%pascal.walkthroughs.title%"
			},
			{
				"description": "%walkthrough.pythonWelcome.description%",
				"id": "pythonWelcome",
				"steps": [
					{
						"description": "%walkthrough.step.python.createPythonFolder.description%",
						"id": "python.createPythonFolder",
						"media": {
							"altText": "%walkthrough.step.python.createPythonFile.altText%",
							"svg": "resources/walkthrough/open-folder.svg"
						},
						"title": "%walkthrough.step.python.createPythonFolder.title%",
						"when": "workspaceFolderCount = 0"
					}
				],
				"title": "%walkthrough.pythonWelcome.title%",
				"when": "workspacePlatform != webworker"
			}
		]
	},
	"workspaces": [
		"workspace/**"
	],
	"workspaces": {
		"packages": [
			"workspace/**"
		]
	}
}